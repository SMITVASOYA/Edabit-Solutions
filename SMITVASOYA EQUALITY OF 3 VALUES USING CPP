#include<iostream>
using namespace std;
int counter[3];

void equal(int a,int b, int c){
	
	int i,j,temp;
	
	if(a==b && a==c){
		counter[0] = 3;
	}
	else if(a==b && a!=c){
		counter[1] = 2;
	}
	else if(a!=b && a==c){
		counter[1] =2;
	}
	else{
		counter[2] = 0;
	}
	for(i=0;i<3;i++)
	{		
		for(j=i+1;j<3;j++)
		{
			if(counter[i]>counter[j])
			{
				temp  =counter[i];
				counter[i]=counter[j];
				counter[j]=temp;
			}
		}
	}
	
}


int main(){
	int a,b,c;
	equal(4,5,6);
	equal(1,1,1);
	equal(2,0,2);

	for(int i=0;i<3;i++){
		cout << counter[i]<<endl;
	}
	
	return 0;
}
